<% include  ./partials/header.ejs %>
<div class="w-full min-h-screen bg-zinc-200 text-zinc-800 pt-1 px-10 py-8">
    <div class="cards flex flex-wrap gap-10">
        <% user.posts.forEach(function(elem){ %>
            <div>
                <div class="card w-32">
                    <div class="w-full  h-48 rounded-lg shadow-md overflow-hidden">
                        <img src="/images/uplods/<%= elem.image %>" alt="" class="object-cover w-full h-full">
                    </div>
                    <h5 class="mt-2 text-sm font-bold mb-2"><%= elem.title %></h5>
                </div>
            </div>
        <% }); %>
    </div>
</div>
<% include  ./partials/footer.ejs %>
